<div class="container mx-auto ">
  <h3 class="text-2xl text-center"><%= @user.name %>'s Gifs</h3>


  <div class="flex flex-wrap justify-center">
    <!-- TIP: Nice way to use a partial. It's expecting it to be in gifs/_gif.html.erb-->
    <%= render @gifs %>
  </div>
</div>



<% @user.gifs.each do |gif| %>
<%= cl_image_tag gif.key, height: 300, width: 400, crop: :fill %>
<% end %>